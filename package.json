{
  "name": "official-turborepo-v1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:nextjs-supabase-auth": "turbo dev --filter=nextjs-supabase-auth",
    "dev:with-supabase-template": "turbo dev --filter=with-supabase-template",
    "dev:better-image-ai": "turbo dev --filter=better-image-ai",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "^2.3.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@9.8.0",
  "workspaces": [
    "apps/chrome-extensions/*",
    "apps/miscellaneous/*",
    "apps/next-14/*",
    "apps/next-15/*",
    "apps/original/*",
    "packages/ai/*",
    "packages/backend/*",
    "packages/core/*",
    "packages/next/*",
    "packages/original/*",
    "packages/react/*"
  ]
}
